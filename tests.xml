  </test>
  <test>
    <id>0012</id>
    <uri>http://0012.iris.test.ing/a#b#c</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0012.iris.test.ing</host>
      <hostname>0012.iris.test.ing</hostname>
      <port></port>
      <pathname>/a</pathname>
      <search></search>
      <hash>#b#c</hash>
    </expected>
