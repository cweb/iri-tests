<tests>
  <test>
    <id>0001</id>
    <uri>http://0001.iris.test.ing/foo/bar&#x0041;</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0001.iris.test.ing</host>
      <hostname>0001.iris.test.ing</hostname>
      <port></port>
      <pathname>/foo/bar&#x0041;</pathname>
      <search></search>
      <hash></hash>
    </expected>
  </test>
  <test>
    <id>0002</id>
    <uri>http://0002.iris.test.ing/foo/bar?query#frag</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0002.iris.test.ing</host>
      <hostname>0002.iris.test.ing</hostname>
      <port></port>
      <pathname>/foo/bar</pathname>
      <search>?query</search>
      <hash>#frag</hash>
    </expected>
  </test>
  <test>
    <id>0003</id>
    <uri>&#32;http://0003.iris.test.ing</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0003.iris.test.ing</host>
      <hostname>0003.iris.test.ing</hostname>
      <port></port>
      <pathname>/</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>leading whitespace</comment>
  </test>
  <test>
    <id>0004</id>
    <uri>http://0004.iris.test.ing/a&#32;b</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0004.iris.test.ing</host>
      <hostname>0004.iris.test.ing</hostname>
      <port></port>
      <pathname>/a b</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>path contains whitespace</comment>
  </test>
  <test>
    <id>0005</id>
    <uri>http://0005.iris.test.ing/a%20b</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0005.iris.test.ing</host>
      <hostname>0005.iris.test.ing</hostname>
      <port></port>
      <pathname>/a%20b</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>path contains escaped whitespace</comment>
  </test>
  <test>
    <id>0006</id>
    <uri>http://0006.iris.test.ing/a%</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0006.iris.test.ing</host>
      <hostname>0006.iris.test.ing</hostname>
      <port></port>
      <pathname>/a%</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>path contains single %</comment>
  </test>
  <test>
    <id>0007</id>
    <uri>http://0007.iris.test.ing&#32;</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0007.iris.test.ing</host>
      <hostname>0007.iris.test.ing</hostname>
      <port></port>
      <pathname>/</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>trailing whitespace</comment>
  </test>
  <test>
    <id>0008</id>
    <uri>http://0008.iris.test.ing/\^`{|}</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0008.iris.test.ing</host>
      <hostname>0008.iris.test.ing</hostname>
      <port></port>
      <pathname>/\^`{|}</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>non-path characters in path</comment>
  </test>
  <test>
    <id>0009</id>
    <uri>http://0009.iris.test.ing/a-umlaut-ä</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0009.iris.test.ing</host>
      <hostname>0009.iris.test.ing</hostname>
      <port></port>
      <pathname>/a-umlaut-ä</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>non-ASCII character in path</comment>
  </test>
  <test>
    <id>0010</id>
    <uri>http://0010.iris.test.ing/a-umlaut-%C3%A4</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0010.iris.test.ing</host>
      <hostname>0010.iris.test.ing</hostname>
      <port></port>
      <pathname>/a-umlaut-%C3%A4</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>percent-encoded non-ASCII character in path</comment>
  </test>
  <test>
    <id>0011</id>
    <uri>http://0011.iris.test.ing/a-umlaut-%c3%a4</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0011.iris.test.ing</host>
      <hostname>0011.iris.test.ing</hostname>
      <port></port>
      <pathname>/a-umlaut-%c3%a4</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>percent-encoded non-ASCII character in path (is the case of percent encoded sequences preserved?)</comment>
  </test>
  <test>
    <id>0012</id>
    <uri>http://0012.iris.test.ing/a#b#c</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0012.iris.test.ing</host>
      <hostname>0012.iris.test.ing</hostname>
      <port></port>
      <pathname>/a</pathname>
      <search></search>
      <hash>#b#c</hash>
    </expected>
    <comment>multiple hash characters</comment>
  </test>
  <test>
    <id>0013</id>
    <uri>http://0013.iris.test.ing/a#b c</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0013.iris.test.ing</host>
      <hostname>0013.iris.test.ing</hostname>
      <port></port>
      <pathname>/a</pathname>
      <search></search>
      <hash>#b c</hash>
    </expected>
    <comment>white space in fragment</comment>
  </test>
  <test>
    <id>0014</id>
    <uri>http://0014.a-umlaut-ä.iris.test.ing/</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0014.a-umlaut-ä.iris.test.ing</host>
      <hostname>0014.a-umlaut-ä.iris.test.ing</hostname>
      <port></port>
      <pathname>/</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>non-ASCII character in host name</comment>
  </test>
  <test>
    <id>0015</id>
    <uri>http://[FEDC:BA98:7654:3210:FEDC:BA98:7654:3210]/</uri>
    <expected>
      <protocol>http:</protocol>
      <host>[FEDC:BA98:7654:3210:FEDC:BA98:7654:3210]</host>
      <hostname>[FEDC:BA98:7654:3210:FEDC:BA98:7654:3210]</hostname>
      <port></port>
      <pathname>/</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>IPv6 address</comment>
  </test>
  <test>
    <id>0016</id>
    <uri>http://a[b].0016.iris.test.ing/</uri>
    <expected>
      <protocol></protocol>
      <host></host>
      <hostname></hostname>
      <port></port>
      <pathname></pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>does not parse according to WEBADDRESSES</comment>
  </test>
  <test>
    <id>0017</id>
    <uri>data:,A%20brief%20note%25foo#bar</uri>
    <expected>
      <protocol>data:</protocol>
      <host></host>
      <hostname></hostname>
      <port></port>
      <pathname>/,A%20brief%20note%25foo</pathname>
      <search></search>
      <hash>#bar</hash>
    </expected>
    <comment>data URI with fragment</comment>
  </test>
  <test>
    <id>0018</id>
    <uri>ical://0018.iris.test.ing/apple-made-this-up</uri>
    <expected>
      <protocol>ical:</protocol>
      <host>0018.iris.test.ing</host>
      <hostname>0018.iris.test.ing</hostname>
      <port></port>
      <pathname>/apple-made-this-up</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>unregistered URI scheme, apparently hierarchical</comment>
  </test>
  <test>
    <id>0019</id>
    <uri>julian://0019.iris.test.ing/julian-made-this-up</uri>
    <expected>
      <protocol>julian:</protocol>
      <host>0019.iris.test.ing</host>
      <hostname>0019.iris.test.ing</hostname>
      <port></port>
      <pathname>/julian-made-this-up</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>unregistered URI scheme, apparently hierarchical</comment>
  </test>
  <test>
    <id>0020</id>
    <uri>opaquelocktoken:5fe6e7e0-1bb3-11df-9f3e-a5b17f9068f4</uri>
    <expected>
      <protocol>opaquelocktoken:</protocol>
      <host></host>
      <hostname></hostname>
      <port></port>
      <pathname>/5fe6e7e0-1bb3-11df-9f3e-a5b17f9068f4</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>maybe not hierarchical?</comment>
  </test>
  <test>
    <id>0021</id>
    <uri>urn:ietf:rfc:3986</uri>
    <expected>
      <protocol>urn:</protocol>
      <host></host>
      <hostname></hostname>
      <port></port>
      <pathname>/ietf:rfc:3986</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>maybe not hierarchical?</comment>
  </test>
  <test>
    <id>0022</id>
    <uri>http://0022.iris.test.ing/a-umlaut/?x=ä&#ä#ä</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0022.iris.test.ing</host>
      <hostname>0022.iris.test.ing</hostname>
      <port></port>
      <pathname>/a-umlaut</pathname>
      <search>?x=ä&</search>
      <hash>#ä#ä</hash>
    </expected>
    <comment>non-ASCII character in query string and fragment</comment>
  </test>
