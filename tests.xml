  </test>
  <test>
    <id>0013</id>
    <uri>http://0013.iris.test.ing/a#b c</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0013.iris.test.ing</host>
      <hostname>0013.iris.test.ing</hostname>
      <port></port>
      <pathname>/a</pathname>
      <search></search>
      <hash>#b c</hash>
    </expected>
    <comment>white space in fragment</comment>
  </test>
  <test>
    <id>0014</id>
    <uri>http://0014.a-umlaut-ä.iris.test.ing/</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0014.a-umlaut-ä.iris.test.ing</host>
      <hostname>0014.a-umlaut-ä.iris.test.ing</hostname>
      <port></port>
      <pathname>/</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>non-ASCII character in host name</comment>
  </test>
  <test>
    <id>0015</id>
    <uri>http://[FEDC:BA98:7654:3210:FEDC:BA98:7654:3210]/</uri>
    <expected>
      <protocol>http:</protocol>
      <host>[FEDC:BA98:7654:3210:FEDC:BA98:7654:3210]</host>
      <hostname>[FEDC:BA98:7654:3210:FEDC:BA98:7654:3210]</hostname>
      <port></port>
      <pathname>/</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>IPv6 address</comment>
  </test>
  <test>
    <id>0016</id>
    <uri>http://a[b].0016.iris.test.ing/</uri>
    <expected>
      <protocol></protocol>
      <host></host>
      <hostname></hostname>
      <port></port>
      <pathname></pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>does not parse according to WEBADDRESSES</comment>
  </test>
