<tests>
  <test>
    <id>0001</id>
    <uri>http://0001.iris.test.ing\foo\bar</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0001.iris.test.ing</host>
      <hostname>0001.iris.test.ing</hostname>
      <port></port>
      <pathname>/foo/bar&#x0041;</pathname>
      <search></search>
      <hash></hash>
    </expected>
  </test>
  <test>
    <id>0002</id>
    <uri>http://0002.iris.test.ing/foo/bar?query#frag</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0002.iris.test.ing</host>
      <hostname>0002.iris.test.ing</hostname>
      <port></port>
      <pathname>/foo/bar</pathname>
      <search>?query</search>
      <hash>#frag</hash>
    </expected>
  </test>
  <test>
    <id>0004</id>
    <uri>http://0004.iris.test.ing/a&#32;b</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0004.iris.test.ing</host>
      <hostname>0004.iris.test.ing</hostname>
      <port></port>
      <pathname>/a b</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>path contains whitespace</comment>
  </test>
  <test>
    <id>0005</id>
    <uri>http://0005.iris.test.ing/a%20b</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0005.iris.test.ing</host>
      <hostname>0005.iris.test.ing</hostname>
      <port></port>
      <pathname>/a%20b</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>path contains escaped whitespace</comment>
  </test>
  <test>
    <id>0006</id>
    <uri>http://0006.iris.test.ing/a%</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0006.iris.test.ing</host>
      <hostname>0006.iris.test.ing</hostname>
      <port></port>
      <pathname>/a%</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>path contains single %</comment>
  </test>
  <test>
    <id>0008</id>
    <uri>http://0008.iris.test.ing/\^`{|}</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0008.iris.test.ing</host>
      <hostname>0008.iris.test.ing</hostname>
      <port></port>
      <pathname>/\^`{|}</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>non-path characters in path</comment>
  </test>
  <test>
    <id>0009</id>
    <uri>http://0009.iris.test.ing/a-umlaut-ä</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0009.iris.test.ing</host>
      <hostname>0009.iris.test.ing</hostname>
      <port></port>
      <pathname>/a-umlaut-ä</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>non-ASCII character in path</comment>
  </test>
  <test>
    <id>0010</id>
    <uri>http://0010.iris.test.ing/a-umlaut-%C3%A4</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0010.iris.test.ing</host>
      <hostname>0010.iris.test.ing</hostname>
      <port></port>
      <pathname>/a-umlaut-%C3%A4</pathname>
      <search></search>
      <hash></hash>
    </expected>
    <comment>percent-encoded non-ASCII character in path</comment>
  </test>
  <test>
    <id>0011</id>
    <uri>http://0011.iris.test.ing/a-umlaut-%c3%a4</uri>
    <expected>
      <protocol>http:</protocol>
      <host>0011.iris.test.ing</host>
      <hostname>0011.iris.test.ing</hostname>
      <port></port>
      <pathname>/a-umlaut-%c3%a4</pathname>
      <search></search>
      <hash></hash>
    </expected>
